{
  "name": "license-reporter",
  "version": "1.4.0",
  "description": "License-reporter gathers license information and reports them in various formats.",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore test/lib/*.js index.js lib/modules/*.js lib/commands/*.js lib/utils/*.js",
    "test": "jest",
    "watch-test": "jest --watch",
    "coverage": "jest --coverage",
    "prepare": "husky install",
    "release": "standard-version",
    "check": "tsc --noEmit true"
  },
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": "^16 || ^14 || ^12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodeshift/license-reporter.git"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "js-yaml": "^4.1.0",
    "js2xmlparser": "^4.0.1",
    "jsonschema": "^1.2.9",
    "license-checker": "^25.0.1",
    "mustache": "^4.2.0",
    "node-project-validator": "^0.1.3",
    "xml2js": "^0.4.23",
    "yargs": "^17.1.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^16.11.11",
    "eslint": "^7.32.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jsdoc": "^3.6.7",
    "proxyquire": "^2.1.3",
    "rewire": "^5.0.0",
    "standard-version": "^9.3.1",
    "typescript": "^4.5.2"
  },
  "preferGlobal": true,
  "bin": {
    "license-reporter": "bin/license-reporter"
  },
  "support": true
}
