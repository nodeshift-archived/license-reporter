{
  "name": "license-reporter",
  "version": "1.3.0",
  "description": "License-reporter gathers license information and reports them in various formats.",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore test/lib/*.js index.js lib/modules/*.js lib/commands/*.js lib/utils/*.js",
    "test": "jest",
    "watch-test": "jest --watch",
    "coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": "^14 || ^12 || ^10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodeshift/license-reporter.git"
  },
  "dependencies": {
    "js-yaml": "^3.14.1",
    "js2xmlparser": "^4.0.1",
    "jsonschema": "^1.2.9",
    "license-checker": "^24.0.1",
    "mustache": "^3.2.1",
    "node-project-validator": "^0.1.3",
    "sync-request": "4.1.0",
    "xml2js": "^0.4.23",
    "yargs": "^13.3.2"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "coveralls": "3.0.0",
    "eslint": "^7.13.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^5.1.3",
    "jest": "^26.5.3",
    "proxyquire": "^2.1.3",
    "rewire": "^5.0.0"
  },
  "preferGlobal": true,
  "bin": {
    "license-reporter": "bin/license-reporter"
  },
  "support": true
}
